[sub_resource type="GDScript" id=5]
script/source = "extends OptionButton


func _ready():
	var ci = load(\"res://images/circle_smooth.png\").get_data()
	var ti = load(\"res://images/triangle_smooth.png\").get_data()
	var ta = load(\"res://images/trapezoid_smooth.png\").get_data()
	var di = load(\"res://images/diamond_smooth.png\").get_data()
	
	ci.resize(64,64)
	ti.resize(64,64)
	ta.resize(64,64)
	di.resize(64,64)
	
	var t_ci = ImageTexture.new()
	var t_ti = ImageTexture.new()
	var t_ta = ImageTexture.new()
	var t_di = ImageTexture.new()
	
	t_ci.create_from_image(ci)
	t_ti.create_from_image(ti)
	t_ta.create_from_image(ta)
	t_di.create_from_image(di)
	
	add_icon_item(t_ci,\"\",0)
	add_icon_item(t_ti,\"\",1)
	add_icon_item(t_ta,\"\",2)
	add_icon_item(t_di,\"\",3)
	
	set_item_metadata(0, \"circle\")
	set_item_metadata(1, \"triangle\")
	set_item_metadata(2, \"trapezoid\")
	set_item_metadata(3, \"diamond\")

"

