[sub_resource type="GDScript" id=2]
script/source = "extends Button

onready var main_menu : CenterContainer = get_tree().get_root().find_node(\"MainMenu\", true, false)
onready var info : AcceptDialog = find_parent(\"CampaignManager\").find_node(\"InfoDialog\", true, false)
onready var campaign_node : CenterContainer = get_tree().get_root().find_node(\"CampaignEditor\", true, false)

func _on_Import_pressed():
	var cb = OS.get_clipboard()
	var decode = JSON.parse(cb)
	if decode.error != OK:
		info.dialog_text = \"Import decoding error!\\n\\n\"
		info.dialog_text += decode.error_string
	else:
		var campaign : Dictionary = decode.result
		# Basic sanity
		if not \"name\" in campaign or not \"missions\" in campaign:
			info.dialog_text = \"Import error!\"
		else:
			info.dialog_text = \"Imported campaign:\\n\\n\" + campaign[\"name\"]
			Global.campaigns[campaign[\"name\"]] = campaign
			campaign_node.flush_campaign_to_disk()
	info.show()
	main_menu.show_menu(\"CampaignManager\")
	
"

