config_version=4

[application]

config/name="Spherifactory"
run/main_scene="res://Title.tscn"
boot_splash/image="res://images/splash.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://images/ico.png"
config/windows_native_icon="res://images/ico.ico"

[autoload]

Global="*res://scripts/Global.gd"
Music="*res://scenes/Music.tscn"
Sfx="*res://scenes/Sfx.tscn"

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[editor_plugins]

enabled=PoolStringArray( "res://addons/NormalMap/plugin.cfg" )

[global]

export=false
addons=true

